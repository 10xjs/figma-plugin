    "@reduxjs/toolkit": "v2.0.0-beta.0",
    "change-case": "^4.1.2",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "use-immer": "^0.9.0",
    "zod": "^3.21.4"
